a, a:active, a:link, a:visited
  text-decoration: none
  color: #069

a:hover
  text-decoration: underline
  color: #C00

#content
  background: url(<%= asset_path "bg_content.gif" %>) left top repeat-x #FFFFFF
  width: 938px !important
  border-left: 1px solid #9c9c9c
  border-right: 1px solid #9c9c9c
  padding: 10px 0px

#textcontent
  padding: 10px !important
  width: 900px !important

#wrapper
  padding-bottom: 10px !important
  background: url(<%= asset_path "bg_content_bottom.gif" %>) center bottom no-repeat

#footer
  text-align: center
  padding: 15px 0px
  color: #666
  font-size: 11px

.hidden
  display: none

.nopadding
  padding: 0px !important

.floatLeft
  float: left

.floatRight
  float: right


